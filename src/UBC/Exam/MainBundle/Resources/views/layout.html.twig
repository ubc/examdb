	    <div class="content expand" role="main">
	    	{% block body %}
	    	{% endblock %}
	    </div>
	    <!-- End of Content Area -->
    </div>
    <!-- Footer Area Unit Menu - Mobile Only -->
    <div id="ubc7-unit-alternate-navigation" style="display:none !important" class="navbar expand visible-phone" role="navigation">
        <div class="navbar-inner expand">
            <div class="container">
                <div class="nav-collapse collapse">
                    <ul class="nav">
		        <!-- **changes here need to be replicated in the main navigation above!** -->
			<li class="{% if app.request.attributes.get('_route') == 'ubc_exam_main_homepage' %}active{% endif %}">
			  <a href="{{ url('ubc_exam_main_homepage') }}">Search Exams</a>
			</li>
                        <li class="{% if app.request.attributes.get('_route') == 'exam_upload' %}active{% endif %}">
			  <a href="{{ url('exam_upload') }}">Upload New</a>
			</li>
